<% content_for('application_header') do -%>
<h1>
<% if @asset.new_record? -%>
  Upload a new asset
<% else -%>
  Edit "<%= @asset.filename %>"
<% end -%>
</h1>
<% end -%>


<%= error_messages_for 'asset' -%>
<% @asset.category_id = params[:category_id] if @asset.new_record? && !params[:category_id].nil? %>
<%= render :partial=>'asset/form' %>


